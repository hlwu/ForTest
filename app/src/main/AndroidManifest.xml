<?xml version="1.0" encoding="utf-8"?>
<manifest package="fortest.hlwu.com.fortest"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".activitylifecycle.LifeCycleActivity"
            android:launchMode="singleTask"
            android:taskAffinity="com.hlwu.test.task1"
            android:configChanges="orientation|screenSize"/>
        
        <activity
            android:name=".activitylifecycle.TransparentActivity"
            android:theme="@style/TransparentTheme"/>

        <activity android:name=".activitylifecycle.NonTransparentActivity"/>

        <activity android:name=".multi_process.NewProcessActivity" android:process=":remoute111"/>

        <service android:name=".aidlAndService.MyTestServiceWithAidl" android:process=":remounte111"/>
    </application>

</manifest>